<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="co.edu.udea.compumovil.gr04_20172.lab4.About_Fragment">

    <!-- TODO: Update blank fragment layout -->
    <android.support.constraint.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="84dp"
            android:layout_height="90dp"
            android:layout_marginBottom="8dp"
            android:layout_marginLeft="8dp"
            android:layout_marginRight="8dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:srcCompat="@mipmap/ic_logo_round"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/textViewJerson" />

        <TextView
            android:id="@+id/TextAbout"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="8dp"
            android:layout_marginLeft="8dp"
            android:layout_marginRight="8dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="16dp"
            android:gravity="center"
            android:text="Acerca de Nosotros"
            android:textSize="25dp"
            android:textColorHighlight="@color/colorLight"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:layout_width="383dp"
            android:layout_height="126dp"
            android:text="@string/about"
            android:layout_marginTop="10dp"
            app:layout_constraintTop_toBottomOf="@+id/textViewAbout"
            android:id="@+id/textView"
            android:layout_marginRight="8dp"
            android:textSize="12dp"
            app:layout_constraintRight_toRightOf="parent"
            android:layout_marginLeft="8dp"
            app:layout_constraintLeft_toLeftOf="parent" />
        <TextView
            android:layout_width="362dp"
            android:layout_height="122dp"
            android:textSize="16dp"
            android:id="@+id/textViewAbout"
            android:text="@string/aboutTextView"
            android:layout_marginLeft="8dp"
            app:layout_constraintLeft_toLeftOf="parent"
            android:layout_marginTop="13dp"
            app:layout_constraintTop_toBottomOf="@+id/TextAbout"
            android:layout_marginRight="8dp"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintHorizontal_bias="0.506"
            tools:layout_editor_absoluteY="60dp"
            tools:layout_editor_absoluteX="3dp" />

        <TextView
            android:id="@+id/textViewElaborada"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Elaborada por:"
            android:layout_marginLeft="8dp"
            app:layout_constraintLeft_toLeftOf="parent"
            android:layout_marginRight="8dp"
            app:layout_constraintRight_toRightOf="parent"
            android:layout_marginTop="10dp"
            app:layout_constraintTop_toBottomOf="@+id/textView" />

        <TextView
            android:id="@+id/textViewDairo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Dairo Arley Garcia Vergara"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/textViewElaborada"
            android:layout_marginLeft="8dp"
            app:layout_constraintLeft_toLeftOf="parent"
            android:layout_marginRight="8dp"
            app:layout_constraintRight_toRightOf="parent" />

        <TextView
            android:id="@+id/textViewJerson"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Jerson López Castaño"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/textViewDairo"
            android:layout_marginRight="8dp"
            app:layout_constraintRight_toRightOf="parent"
            android:layout_marginLeft="8dp"
            app:layout_constraintLeft_toLeftOf="parent" />

    </android.support.constraint.ConstraintLayout>




</FrameLayout>
